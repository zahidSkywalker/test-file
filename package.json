{
  "name": "reseller-ecommerce",
  "version": "1.0.0",
  "description": "Modern reselling e-commerce platform with professional UI/UX",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "node build.js",
    "build:css": "tailwindcss -i ./public/css/input.css -o ./public/css/output.css --minify",
