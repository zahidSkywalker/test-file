{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/", "destination": "/public/index.html" },
    { "source": "/products", "destination": "/public/products.html" },
    { "source": "/about", "destination": "/public/about.html" },
    { "source": "/contact", "destination": "/public/contact.html" },
    { "source": "/services", "destination": "/public/services.html" },
    { "source": "/blog", "destination": "/public/blog.html" },
    { "source": "/checkout", "destination": "/public/checkout/index.html" },
    { "source": "/checkout/:path*", "destination": "/public/checkout/:path*" },
    { "source": "/css/:path*", "destination": "/public/css/:path*" },
    { "source": "/js/:path*", "destination": "/public/js/:path*" },
    { "source": "/images/:path*", "destination": "/public/images/:path*" },
    { "source": "/:path*.html", "destination": "/public/:path*.html" },
    { "source": "/:path*", "destination": "/public/:path*" }
  ]
}